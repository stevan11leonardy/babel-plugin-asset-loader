{
  "name": "babel-plugin-asset-loader",
  "version": "0.1.1",
  "description": "Help to load your asset using babel",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "clean": "rimraf lib coverage .nyc_output",
    "coverage": "cross-env NODE_ENV=test nyc report --reporter=lcov",
    "coveralls": "cross-env NODE_ENV=test nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint --ext js src test/**/*.spec.*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevan11leonardy/babel-plugin-asset-loader.git"
  },
  "keywords": [
    "nextjs",
    "babel",
    "import",
    "loader",
    "static",
    "images",
    "cdn",
    "sse",
    "isomorphic"
  ],
  "author": "Stevan Leonardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevan11leonardy/babel-plugin-asset-loader.git/issues"
  },
  "homepage": "https://github.com/stevan11leonardy/babel-plugin-asset-loader.git#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.0",
    "eslint": "^4.1.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.2",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "big.js": "^5.0.3",
    "fs-extra": "^4.0.2",
    "mime": "^2.0.3"
  }
}
